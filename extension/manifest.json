{
  "manifest_version": 2,

  "name": "Q",
  "version": "0.1",
  "description": "Junction Project",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["background", "bookmarks", "cookies", "notifications", "tabs", "<all_urls>"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}